{
  "name": "project_3",
  "version": "1.0.0",
  "description": "Final UC Berkeley Fullstack Project",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spinglez/project_3.git"
  },
  "author": "Tam, James, Matt, Nareg, Christian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Spinglez/project_3/issues"
  },
  "homepage": "https://github.com/Spinglez/project_3#readme",
  "dependencies": {
    "auth0-js": "^9.10.0",
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "history": "^4.7.2",
    "if-env": "^1.0.4",
    "imdb-prime": "^1.0.0",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.4.17",
    "morgan": "^1.9.1"
  }
}
